<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='modulehelper'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
  
   <inherits name="com.google.gwt.logging.Logging"/>
  
   <set-property name="user.agent" value="gecko1_8, safari" /> 
   <!--  <set-property name="user.agent" value="gecko1_8, safari, ie8" />-->


  <!-- Other module inherits   -->
    
   <!-- Include GWTUpload library -->
  <inherits name="gwtupload.GWTUpload"/>
  <!-- Include advanced widget -->
  <inherits name='org.gwt.advanced.Grid'/>
  <!-- Load dynamically predefined styles in the library when the application starts -->
  <stylesheet src="Upload.css"/>
                                     

  <!-- Specify the app entry point class.                         -->
  <entry-point class='fr.tsadeo.app.gwt.modulehelper.client.ModuleHelperEntryPoint'/>
  
  <stylesheet src="advanced/themes/default/theme.css"/>
  <stylesheet src="css/ModuleHelperView.css" />

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>


     <!-- logging -->
    <set-property name="gwt.logging.logLevel" value="CONFIG"/>           # To change the default logLevel
    <set-property name="gwt.logging.enabled" value="FALSE"/>            # To disable logging
    <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>  # To disable a default Handler
    
</module>
